{
	"$schema": "https://biomejs.dev/schemas/2.3.6/schema.json",
	"linter": {
		"rules": {
			"correctness": {
				"noUnusedVariables": "error",
				"noUnusedImports": "error",
				"useJsxKeyInIterable": "off"
			},
			"style": {
				"useEnumInitializers": "off"
			},
			"suspicious": {
				"noExplicitAny": "off"
			}
		}
	}
}
